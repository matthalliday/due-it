<%= form_for @project do |f| %>
	<%= render 'partials/errors', object: @project %>

	<div class="field">
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<%= f.label :description %><br />
		<%= f.text_area :description %>
	</div>
	<div class="field submit">
		<br />
		<%= f.submit %>
	</div>
<% end %>